{
    "name": "spiral/queue",
    "type": "library",
    "description": "Spiral: Jobs and Queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        },
        {
            "name": "Pavel Buchnev",
            "email": "butschster@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "spiral/core": "^3.0",
        "spiral/snapshots": "^3.0",
        "spiral/attributes": "^3.0",
        "doctrine/inflector": "^1.4|^2.0",
        "ramsey/uuid": "^4.2.3",
        "psr/event-dispatcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Queue\\": "tests"
        }
    },
    "require-dev": {
        "doctrine/annotations": "^1.12",
        "phpunit/phpunit": "^9.5.20",
        "mockery/mockery": "^1.5",
        "spiral/boot": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.14.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
